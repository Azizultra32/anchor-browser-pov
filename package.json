{
  "name": "anchor-browser-poc",
  "version": "1.0.0",
  "description": "This is a minimal proof-of-concept showing: - A Chrome MV3 extension that injects a Ghost Overlay UI - A PHI-safe DOM mapper (labels/roles/selectors only) - A local agent (Node/Express) that receives the map and returns a deterministic fill plan - A demo HTML page to test against",
  "main": "index.js",
  "bin": {
    "codex": "bin/codex.mjs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./anchor-start.sh",
    "stop": "./anchor-stop.sh",
    "ready": "node bin/codex.mjs ready",
    "status": "node bin/codex.mjs status",
    "smoke": "node bin/codex.mjs smoke",
    "kill": "node bin/codex.mjs kill",
    "mcp": "node bin/codex.mjs mcp --stdio",
    "anchor:status": "./anchor-status.sh",
    "anchor:smoke": "node scripts/anchor-smoke.mjs",
    "smoke:old": "node scripts/smoke.mjs",
    "health": "node scripts/mcp-health-check.mjs",
    "dashboard": "node anchor-server.mjs",
    "mcp:start": "scripts/start-chrome-mcp.sh",
    "mcp:stop": "scripts/stop-chrome-mcp.sh",
    "mcp:status": "scripts/status-chrome-mcp.sh",
    "mcp:map": "node mcp/anchor-server.mjs anchor_map_page http://localhost:8788/ehr.html",
    "mcp:plan": "node mcp/anchor-server.mjs anchor_plan_fill http://localhost:8788/ehr.html 'MCP test note'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "chrome-remote-interface": "^0.33.3",
    "commander": "^12.1.0",
    "puppeteer-core": "^24.30.0",
    "express": "^4.19.2",
    "cors": "^2.8.5"
  }
}
